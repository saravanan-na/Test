<?xml version="1.0" encoding="UTF-8"?><project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>com.photon.phresco</groupId>
  <artifactId>GitNone</artifactId>
  <version>1.0.0-SNAPSHOT</version>
    <scm>
        <connection>https://github.com/saravanan-na/Test.git</connection>
        <developerConnection>scm:git:https://github.com/saravanan-na/Test.git</developerConnection>
        <url>scm:git:https://github.com/saravanan-na/Test.git</url>
    </scm>
  <packaging>phresco</packaging>
  <name>GitNone</name>

  <properties>
        <phresco.test.dir>test</phresco.test.dir>
        <source.pom>pom.xml</source.pom>
        <phresco.env.config.xml>/src/main/resources/phresco-env-config.xml</phresco.env.config.xml>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <phresco.unitTest.report.dir>/do_not_checkin/target/surefire-reports</phresco.unitTest.report.dir>
        <phresco.unitTest.testsuite.xpath>/testsuite</phresco.unitTest.testsuite.xpath>
        <phresco.unitTest.testcase.path>/testcase</phresco.unitTest.testcase.path>
        <phresco.manualTest.report.dir>/${phresco.test.dir}/manual</phresco.manualTest.report.dir>
        <phresco.functionalTest.selenium.tool>webdriver</phresco.functionalTest.selenium.tool>
        <phresco.functionalTest.dir>/${phresco.test.dir}/functional</phresco.functionalTest.dir>
        <phresco.functionalTest.report.dir>/${phresco.test.dir}/functional/target/surefire-reports</phresco.functionalTest.report.dir>
        <phresco.functionalTest.testsuite.xpath>/testsuite</phresco.functionalTest.testsuite.xpath>
        <phresco.functionalTest.testcase.path>/testcase</phresco.functionalTest.testcase.path>
        <phresco.functionalTest.adapt.config>/${phresco.test.dir}/functional/src/test/java/phresco/testcases/phresco-env-config.xml</phresco.functionalTest.adapt.config>
        <phresco.performanceTest.dir>/${phresco.test.dir}/performance</phresco.performanceTest.dir>
        <phresco.performanceTest.report.dir>/${phresco.test.dir}/performance/dir_type/results/jmeter</phresco.performanceTest.report.dir>
        <phresco.loadTest.dir>/${phresco.test.dir}/load</phresco.loadTest.dir>
        <phresco.loadTest.report.dir>/${phresco.test.dir}/load/dir_type/results/jmeter</phresco.loadTest.report.dir>
        <phresco.eclipse>true</phresco.eclipse>
        <phresco.src.repo.url>https://github.com/saravanan-na/Test.git</phresco.src.repo.url>
    </properties>

  <build>
	<directory>do_not_checkin/target</directory>
	<finalName>HelloWorld</finalName>
	<plugins>
		<plugin>
			<groupId>com.photon.phresco.plugins</groupId>
            <artifactId>phresco-maven-plugin</artifactId>
			<version>3.2.0.8003</version>
			<extensions>true</extensions>
		</plugin>
		 <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jar-plugin</artifactId>
        <version>2.4</version>
        <configuration>
                    <archive>
                        <manifest>
                            <addClasspath>true</addClasspath>
                            <mainClass>com.photon.phresco.HelloWorld</mainClass>
            </manifest>
          </archive>
                </configuration>
      </plugin>
      <plugin>
	<groupId>org.apache.maven.plugins</groupId>
	<artifactId>maven-surefire-plugin</artifactId>
	<version>2.16</version>
	 <dependencies>
	  <dependency>
	   <groupId>org.apache.maven.surefire</groupId>
	   <artifactId>surefire-junit47</artifactId>
	   <version>2.16</version>
	 </dependency>
        </dependencies>
    </plugin>
    </plugins>
    <pluginManagement>
      <plugins>
        <plugin>
          <artifactId>maven-compiler-plugin</artifactId>
          <configuration>
                        <source>1.5</source>
                        <target>1.5</target>
                    </configuration>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>
  <profiles>
        <profile>
            <id>ci</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>
            <build>
		        <plugins>
					<plugin>
						<artifactId>maven-clean-plugin</artifactId>
						<version>2.5</version>
						<executions>
						  <execution>
							<id>default-clean</id>
							<phase>pre-clean</phase>
							<goals>
							  <goal>clean</goal>
							</goals>
						  </execution>
						</executions>
						<configuration>
                            <excludeDefaultDirectories>true</excludeDefaultDirectories>
                            <filesets>
                                <fileset>
                                    <directory>do_not_checkin/build</directory>
                                    <includes>
                                        <include>**/*</include>
							  </includes>
                                    <followSymlinks>false</followSymlinks>
							</fileset>
                                <fileset>
                                    <directory>do_not_checkin/archives</directory>
                                    <includes>
                                        <include>**/*</include>
                    </includes>
                                    <followSymlinks>false</followSymlinks>
              </fileset>
						  </filesets>
                        </configuration>
					  </plugin>
		        </plugins>
            </build>
        </profile>
    </profiles>
    <repositories>
        <repository>
            <id>photon</id>
            <url>http://172.16.17.226:8080/repository/content/groups/public/</url>
        </repository>
    </repositories>
</project>
